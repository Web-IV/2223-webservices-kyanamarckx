    "begin": "nodemon ./dist/src/index.js",
    "build": "tsc",
    "push": "npx prisma db push",
    "seed": "npx prisma db seed",
    "generate": "npx prisma generate",
    "reset": "npx prisma migrate reset --force --skip-seed",
